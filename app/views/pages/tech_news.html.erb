<div class="inner cover">
	<h1 class="cover-heading">Technology News</h1>
	<div class="row">
		<% @tweets.each do |tweet| %>
			<div class="col-md-6">
				<div class="tech-news-tweet">
					<div class="card">
						<%= twitter_parser(tweet) %>
					</div>
				</div>
			</div>
		<% end %>
	</div>
</div>